{
    "swagger": "2.0",
    "info": {
        "description": "基于AI的角色扮演聊天系统API文档",
        "title": "AI角色扮演聊天API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
